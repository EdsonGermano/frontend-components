{
  "name": "react.socrata.numberfilter",
  "version": "1.0.0",
  "description": "",
  "main": "./react.socrata.autocomplete.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jquery": "^2.1.4",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.18.0",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "node-sass": "^3.3.2",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "settings": {
      "ecmascript": 6,
      "jsx": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": 1,
      "consistent-return": 0,
      "dot-location": [
        1,
        "object"
      ],
      "dot-notation": [
        1,
        {
          "allowKeywords": false
        }
      ],
      "eol-last": 1,
      "indent": 0,
      "new-cap": [
        1,
        {
          "newIsCapExceptions": [
            "pbf"
          ]
        }
      ],
      "no-console": 0,
      "no-extend-native": 2,
      "no-extra-semi": 1,
      "no-shadow": [
        2,
        {
          "hoist": "all"
        }
      ],
      "no-spaced-func": 2,
      "no-trailing-spaces": 2,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "no-use-before-define": 0,
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "space-after-keywords": [
        2,
        "always"
      ],
      "space-before-blocks": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "space-infix-ops": 2,
      "strict": 2
    },
    "globals": {
      "$": true,
      "_": true,
      "angular": true,
      "blist": true,
      "chroma": true,
      "currentUser": true,
      "d3": true,
      "datasetMetadata": true,
      "domainMetadata": true,
      "jQuery": true,
      "L": true,
      "Modernizr": true,
      "moment": true,
      "pageMetadata": true,
      "pbf": true,
      "Rx": true,
      "ss": true,
      "socrata": true,
      "VBArray": true,
      "VectorTile": true
    }
  }
}
